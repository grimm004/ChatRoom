<Page x:Class="ChatRoom.Pages.RoomList"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChatRoom.Pages"
      mc:Ignorable="d"
      Title="Room Browser">
    <Grid>
        <Grid x:Name="serverListGrid" Margin="0,0,0,40" VerticalAlignment="Top" HorizontalAlignment="Stretch"/>
        <Button x:Name="backButton" Content="Back" Margin="0,249,0,0" Click="backButton_Click" HorizontalAlignment="Stretch" VerticalAlignment="Bottom"/>
        <Button x:Name="refreshButton" Content="Refresh List" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" Margin="0,0,0,20" Click="refreshButton_Click"/>
    </Grid>
</Page>
